{
  "manifest_version": 2,
  "name": "List of Trust",
  "version": "1.1",
  "description": "Assign values to opened URLs",
  "permissions": ["webNavigation", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}

